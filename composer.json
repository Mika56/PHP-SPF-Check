{
    "name": "mika56/spfcheck",
    "description": "Checks an IP address against a domain's SPF record",
    "type": "library",
    "authors": [
        {
            "name": "Mikael Peigney",
            "email": "mikael.56@live.fr"
        },
        {
            "name": "Brian Tafoya",
            "email": "btafoya@briantafoya.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/http-foundation": "2.8.* || ^3.0 || ^4.0 || ^5.0 || ^6.0 || ^7.0",
        "symfony/polyfill-php80": "^1.26"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^4.4||^5.4||^6.0",
        "symfony/phpunit-bridge": "^6.1"
    },
    "suggest": {
        "mika56/spfcheck-dns-direct": "Allow using a custom DNS server instead of system's"
    },
    "autoload": {
        "psr-4": {
            "Mika56\\SPFCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mika56\\SPFCheck\\Test\\": "tests"
        }
    },
    "license": "MIT",
    "keywords": ["spf", "email"]
}
